public class LianXi001 {
    public static void main(String[] args) {
        int a = 1;
        int b = 2;
        int c = 3;
        int d = 5;
        System.out.println("a+b="+(a+b));
        System.out.println("d-a="+(d-a));
        System.out.println("b*c="+b*c);
        System.out.println("d/a="+d/a);
    }
}


