public class LianXi006 {
    public static void main(String[] args) {
        int baozi = 2;
        boolean baoziGangChuLu = true;
        if (baoziGangChuLu) {
            baozi = baozi + 5;
            System.out.println("包子刚出炉，买了"+baozi+"个");
        }else {
            System.out.println("包子没有刚出炉,所以买了"+baozi+"个");
        }
    }
}
